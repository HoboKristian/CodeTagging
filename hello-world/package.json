{
    "name": "hello-world",
    "displayName": "Hello World",
    "description": "test",
    "version": "0.0.1",
    "publisher": "pboutet",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.serialize"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.serialize",
                "title": "Serialize"
            },
            {
                "command": "extension.load",
                "title": "Load"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/lodash": "^4.14.104",
        "lodash": "^4.17.5",
        "serializer.ts": "0.0.12"
    }
}
